[config]
input_sources=yellowstone_sources_{GFSVER}
fcst_catalog=comm_hist
archive=hpss:{SUBEXPT}/{out_prefix}.tar

[dir]
CDNOSCRUB=/glade/p/work/{ENV[USER]}/noscrub
CDSCRUB=/glade/scratch/{ENV[USER]}/pytmp
CDSAVE=/glade/p/work/{ENV[USER]}
syndat=/glade/p/work/strahan/SYNDAT-PLUS

[holdvars]
WHERE_AM_I=yellowstone
NPROCS_A_NOIO=184
NPROCS_C=1
NPROCS_O=9

[wrfexe]
nio_groups=3
nio_tasks_per_group=6
poll_servers=yes
nproc_x=-1
nproc_y=-1

[runwrf]
nio_groups=4
nio_tasks_per_group=4
poll_servers=yes
nproc_x=16
nproc_y=28
wrf_ranks=464

