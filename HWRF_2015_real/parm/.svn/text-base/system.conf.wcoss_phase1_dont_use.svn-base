[config]
disk_project=hwrfv3
input_sources=wcoss_sources_{GFSVER}
fcst_catalog=wcoss_fcst_{GFSVER}
archive=hpss:{SUBEXPT}/{out_prefix}.tar

[dir]
utilexec=/nw{ENV[envir|-prod]}/util/exec
utilscript=/nw{ENV[envir|-prod]}/util/exec
CDNOSCRUB=/hwrf/noscrub/{ENV[USER]}
CDSCRUB=/ptmpd2/{ENV[USER]}
CDSAVE=/hwrf/save/{ENV[USER]}
syndat=/com/arch/prod/syndat

[holdvars]
WHERE_AM_I=wcoss
WHICH_JET=none
NPROCS_A_NOIO=184
NPROCS_C=1
NPROCS_O=9

[hwrfdata]
inputroot=/hwrf2/noscrub/input/hwrfdata/

[wrfexe]
nio_groups=3
nio_tasks_per_group=6
poll_servers=yes
nproc_x=-1
nproc_y=-1

[runwrf]
nio_groups=4
nio_tasks_per_group=4
poll_servers=yes
nproc_x=16
nproc_y=28
wrf_ranks=464

