%include <head.h>
%include <envir-p2.h>

# EXPORT list here
set -x
export job=hwrf%NUM%_gempak_meta_%CYC%
export PDY=${PDY:-%PDY:%}
export cyc=%CYC%
export storm_num=%NUM%

# CALL executable job script here

# versions file for hwrf sets $hwrf_ver and $code_ver
VERSION_FILE=${NWROOT}/versions/hwrf.ver
if [ -f $VERSION_FILE ]; then
  . $VERSION_FILE
else
  ecflow_client --msg="***JOB ${ECF_NAME} ERROR: Version File $VERSION_FILE does not exist ***"
  ecflow_client --abort
  exit
fi

${NWROOT}/hwrf.${hwrf_ver}/jobs/JHWRF_GEMPAK_META

if [ $? -ne 0 ]; then
  ecflow_client --msg="***JOB ${ECF_NAME} ERROR RUNNING J-SCRIPT ***"
  ecflow_client --abort
  exit
fi

