<task name="post_helper_E#ENS#" maxtries="&MAX_TRIES;">
  
  <command>&PRE; &EXhwrf;/exhwrf_post.py</command>
  <jobname>hwrf_post_helper_&SID;_<cyclestr>@Y@m@d@H</cyclestr>_E#ENS#</jobname>
  <account>&ACCOUNT;</account>
  <queue>&PE;</queue>
  <cores>24</cores>
  <envar>
    <name>TOTAL_TASKS</name>
    <value>24</value>
  </envar>
  <walltime>&POST_WALLTIME;</walltime>
  <memory></memory>
  <stdout><cyclestr>&WORKhwrf;/hwrf_post_helper.out</cyclestr></stdout>
  <stderr><cyclestr>&WORKhwrf;/hwrf_post_helper.err</cyclestr></stderr>

  &ENV_VARS;
  &RESERVATION;
  &CORES_EXTRA;
  &MPI_EXTRA;

  <dependency>
    <taskdep task="unpost_E#ENS#"/>
  </dependency>

</task>
