<task name="unpost_E#ENS#" maxtries="&MAX_TRIES;">
 
  <command>&PRE; &EXhwrf;/exhwrf_unpost.py</command>
  <jobname>hwrf_unpost_&SID;_<cyclestr>@Y@m@d@H</cyclestr>_E#ENS#</jobname>
  <account>&ACCOUNT;</account>
  <queue>&SERIAL;</queue>
  <cores>1</cores>
  <envar>
    <name>TOTAL_TASKS</name>
    <value>1</value>
  </envar>
  <walltime>00:05:00</walltime>
  <memory>1G</memory>
  <stdout><cyclestr>&WORKhwrf;/hwrf_unpost.out</cyclestr></stdout>
  <stderr><cyclestr>&WORKhwrf;/hwrf_unpost.err</cyclestr></stderr>

  &ENV_VARS;
  &RESERVATION;
  &CORES_EXTRA;
  &SERIAL_EXTRA;

  <dependency>
    <or>
      &forecast_complete;
      &forecast_running;
    </or>
  </dependency>

</task>
