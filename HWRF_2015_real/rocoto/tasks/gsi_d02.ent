<task name="gsi_d02_E#ENS#" maxtries="&MAX_TRIES;">
  
  <command>&PRE; &EXhwrf;/exhwrf_gsi.py</command>
  <jobname>hwrf_gsi_d02_&SID;_<cyclestr>@Y@m@d@H</cyclestr>_E#ENS#</jobname>
  <account>&ACCOUNT;</account>
  <queue>&PE;</queue>
  <nodes>&GSI_D02_NODES;</nodes>
  <envar>
    <name>TOTAL_TASKS</name>
    <value>&GSI_D02_TASKS;</value>
  </envar>
  <envar>
    <name>GSI_DOMAIN</name>
    <value>d02</value>
  </envar>
  <walltime>&GSI_D02_WALLTIME;</walltime>
  <memory></memory>
  <stdout><cyclestr>&WORKhwrf;/hwrf_gsi_d02.out</cyclestr></stdout>
  <stderr><cyclestr>&WORKhwrf;/hwrf_gsi_d02.err</cyclestr></stderr>

  &ENV_VARS;
  &RESERVATION;
  &NODES_EXTRA;
  &MPI_EXTRA;

  <envar>
      <name>OMP_NUM_THREADS</name>
      <value>&GSI_D02_THREADS;</value>
  </envar>

  <dependency>
    <and>
      <metataskdep metatask="meta_relocate_gdas1_E#ENS#"/>
      <taskdep task="init_GFS_0_E#ENS#"/>
      <streq><left>&RUN_GSI;</left><right>YES</right></streq>
      <taskdep task="bufrprep_E#ENS#"/>
      <or>
        <strneq><left>&DA_ENSEMBLE;</left><right>YES</right></strneq>
        <taskdep task="ensda_output" cycle_offset="-6:00:00"/>
        <not>
          <cycleexistdep cycle_offset="-6:00:00"/>
        </not>
        <sh>grep 'RUN_ENSDA=NO' <cyclestr offset="-6:00:00">&COMhwrf;/&STORMLABEL;.run_ensda</cyclestr></sh>
      </or>
      <sh>grep 'run_gsi_d02=YES' <cyclestr>&COMhwrf;/&STORMLABEL;.gsi_status</cyclestr></sh>
    </and>
  </dependency>

</task>
