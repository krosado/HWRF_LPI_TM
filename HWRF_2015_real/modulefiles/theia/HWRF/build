proc ModulesHelp { } {
    puts stderr "Set environment veriables for compiling HWRF"
}
module-whatis " HWRF hurricane model prerequisites"

set ver 9.0.0

module purge

# Newer version of compiler and MPI than what is presently the
# default:
module load intel/15.1.133
module load impi/5.0.2.044
module load pnetcdf/1.5.0-impi
module load netcdf/4.3.0
module load rocoto/1.2
module load nco/4.4.5
module load hpss
