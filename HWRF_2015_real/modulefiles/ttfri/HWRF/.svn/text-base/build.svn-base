## 2015 HWRF TTFRI prerequisite module
#############################################################
proc ModulesHelp { } {
    puts stderr "Set environment veriables for compiling HWRF"
}
module-whatis " HWRF hurricane model prerequisites"

set ver 9.0.0

# Known conflicts
conflict HWRF

# We'll be using non-default versions of everything so make sure we
# unload older versions of those modules first:
module unload cdo
module unload nco
module unload opengrads
module unload ncl_ncarg
module unload netcdf-4.1.3
module unload mvapich2-1.8

# Newer version of compiler and MPI than what is presently the
# default:
module load mvapich2-1.8/intel-12.1
module load netcdf-4.1.3/intel-12.1
module load ncl_ncarg/6.0.0
module load opengrads/2.0.1.oga.1
module load nco/4.2.1
module load cdo/1.5.6.1
